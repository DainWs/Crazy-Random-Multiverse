{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  
  "applicationId": "com.dainws.monkeys-with-minigun.crm-view-nodejs",
  "version": "1.0.0",
  "defaultMode": "window",
  
  "port": 14023,
  "enableServer": true,
  "enableNativeAPI": true,
  
  "url": "/",
  "documentRoot": "/data/",
  "nativeBlockList": [],
  "nativeAllowList": [
    "app.*",
    "os.*",
    "window.*",
    "filesystem.*",
    "storage.*",
    "debug.log"
  ],
  
  "tokenSecurity": "one-time",
  "exportAuthInfo": false,
  "enableExtensions": false,
  "extensions": [],


  "logging": {
    "enabled": true,
    "writeToLogFile": true,
    "serverHeaders": {}
  },
  "globalVariables": {
    "profile": "development"
  },

  "modes": {
    "window": {
      "title": "Crazy Random Multiverses",
      
      "width": 1280,
      "height": 720,
      "maxWidth": 1280,
      "minWidth": 1280,
      "maxHeight": 720,
      "minHeight": 720,

      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/data/logo.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "profile": "development"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    }
  },
  "cli": {
    "binaryName": "Crazy Random Multiverses",
    "resourcesPath": "/data/",
    "extensionsPath": "/libs/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}