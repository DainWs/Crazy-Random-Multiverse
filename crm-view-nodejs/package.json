{
  "name": "crm-view-nodejs",
  "version": "1.0.0",
  "description": "CRM Game gui made with Electron",
  "author": "DainWs",
  "main": "dist/electron.js",
  "scripts": {
    "vue:lint": "vue-cli-service lint",
    "vue:build": "vue-cli-service build",
    "vue:serve": "vue-cli-service serve",
    "sass": "sass --watch src/assets/styles:dist"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.1.6",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "build": {
    "appId": "com.dainws.monkeys-with-minigun.crm-view-nodejs",
    "productName": "Crazy Random Multiverses",
    "copyright": "Copyright Â© 2022 ${author}",
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true
    },
    "files": [
      "dist/**/*"
    ],
    "extraResources": [
      "defaults/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "keywords": [
    "CRM",
    "Games",
    "Cards"
  ],
  "license": "ISC"
}
